{
  "custom_bonifiche": {
    "filename": {
      "regex": "^([A-Za-z ]+)\\_([0-9a-z]+)\\_([A-Za-z ]+)\\_([0-9]{4})\\.txt$",
      "elements": [ "azienda", "idfield", "crop", "year" ]
    },
    "content": {
      "fields": [ "longitud", "latitud", "distanza", "flusso", "umidita", "larghezza", "area", "resa", "data", "proteine", "amido", "ndf", "adf", "sost_secca" ],
      "caption": true,
      "format": "txt",
      "sep": ","
    },
    "options": {
      "isdir": true
    }
  },
  "custom_ibf": {
    "filename": {
      "regex": "^[A-Za-z]+\\_id\\_geom\\_([0-9a-z]+).shp$",
      "elements": [ "idfield" ]
    },
    "content": {
      "fields": {
        "regex": ["((RESAKG)|(YIELD_KG))", "((UMIDITA)|(MOISTURE))", "((AREA)|(AREA_MQ))", "anno", "raccolto" ],
        "name": ["yield", "humidity", "year", "date", "crop", "yield_dry", "speed", "rel_width" ]
      },
      "caption": true,
      "format": "shp",
      "sep": ","
    },
    "options": {
      "isdir": false
    }
  },
  "johndeere": null
}
